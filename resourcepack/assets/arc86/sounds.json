	"s5": {
		"sounds": ["arc86:s5"],
		"subtitle": "Distant whispering",
		"id": 1,
		"absub": {
			"points": {
				"1520": { "person": "Fiorillo", "msg": "Preston?" },
				"1600": { "person": "Fiorillo", "msg": "Preston..." },
				"1640": {
					"person": "Fiorillo",
					"msg": "Preston, get the hell over here."
				},
				"1660": {
					"person": "Fiorillo",
					"msg": "I told you 'Wake up on time, cmon'"
				},
				"1700": {
					"person": "Fiorillo",
					"msg": "and you fall right back to sleep again."
				},
				"1760": {
					"person": "Fiorillo",
					"msg": " I don’t even know why I bother."
				},
				"1800": { "person": "Fiorillo", "msg": "Hurry up!" },
				"1840": {
					"person": "Fiorillo",
					"msg": "Scientific atrium. Or else Heermann’ll be pissed."
				},
				"1900": { "person": "Fiorillo", "msg": "Again!" },
				"1920": { "person": "Steven", "msg": "Oh well, Preston -" },
				"1960": { "person": "Steven", "msg": "This is Steven" },
				"1980": { "person": "Steven", "msg": "with a V." },
				"2000": {
					"person": "Steven",
					"msg": "I, ugh, I think I might have left my medication"
				},
				"2060": {
					"person": "Steven",
					"msg": "...in my room, can you, ugh"
				},
				"2120": { "person": "Steven", "msg": "Can you bring that?" },
				"2160": { "person": "Steven", "msg": "Thanks." }
			}
		}
	},
