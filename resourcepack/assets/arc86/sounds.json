	"voicelogs.endler": {
		"sounds": ["arc86:voicelogs/endler"],
		"id": 6,
		"endTicks": 1140,
		"absub": {
			"points": {
				"20": {
					"person": "Endler",
					"msg": "Personal log. Fourth of April."
				},
				"82": {
					"person": "Endler",
					"msg": "Had an Osprey fail to take off today."
				},
				"140": {
					"person": "Endler",
					"msg": "Something went off with the left-side tiltrotor."
				},
				"200": {
					"person": "Endler",
					"msg": "That makes, what, one hundred and forty-seven, no."
				},
				"320": {
					"person": "Endler",
					"msg": "One hundred and forty-eight days without incident here at Site-86."
				},
				"422": { "person": "Endler", "msg": "Until now." },
				"462": {
					"person": "Endler",
					"msg": "Pfft. Well, if that is the worst that it is going to get, I'll take it."
				},
				"580": {
					"person": "Endler",
					"msg": "It is all peace and quiet here in Seattle."
				},
				"712": {
					"person": "Endler",
					"msg": "I don't like it. They say it is darkest before the dawn, but they also say it is calm before the storm."
				},
				"904": {
					"person": "Endler",
					"msg": "I heard ICR 47 may have a lead on to something."
				},
				"1016": {
					"person": "Endler",
					"msg": "I hope it's the former of the two, and not the latter."
				}
			}
		}
	},
